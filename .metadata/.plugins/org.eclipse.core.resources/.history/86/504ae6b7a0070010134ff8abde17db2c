package Operater;

public class OperaterEx2 {

	public static void main(String[] args) {
		//비교 연산자

		System.out.println("*****비교 연산*****");
		// "==": 같다, "!=": 같지 않다
		int n1 = 10;
		int n2 = 20;
		
		System.out.println(n1 >= n2);
		System.out.println(n1 <= n2);	
		System.out.println(n1 == n2);
		System.out.println(n1 != n2);
		System.out.println();
		System.out.println("*****논리 연산*****");
		
		System.out.println((n1 >= n2)&&(n1 == n2));
		
		
		System.out.println((n1 >= n2)||(n1 == n2));
		
		
		
		
		
		
		
		
		
		
		
	}

}
